4
(
    hotWall
    {
        type            wall;
        inGroups        List<word> 1(wall);
        nFaces          40;
        startFace       3120;
    }
    coldWall
    {
        type            wall;
        inGroups        List<word> 1(wall);
        nFaces          40;
        startFace       3160;
    }
    adiabaticWalls
    {
        type            wall;
        inGroups        List<word> 1(wall);
        nFaces          80;
        startFace       3200;
    }
    frontAndBack
    {
        type            empty;
        inGroups        List<word> 1(empty);
        nFaces          3200;
        startFace       3280;
    }
